{
  "name": "country-quiz",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint:eslint": "eslint .",
    "lint:eslint:fix": "pnpm run lint:eslint --fix",
    "lint:stylelint": "stylelint '**/*.{css,scss}'",
    "lint:stylelint:fix": "pnpm run lint:stylelint --fix",
    "lint": "pnpm run lint:eslint && pnpm run lint:stylelint",
    "lint:fix": "pnpm run lint:eslint:fix && pnpm run lint:stylelint:fix",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "check": "pnpm run lint && pnpm run format:check",
    "preview": "vite preview"
  },
  "dependencies": {
    "@tailwindcss/vite": "4.1.7",
    "@tanstack/react-query": "5.77.0",
    "@tanstack/react-query-devtools": "5.77.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tailwind-merge": "3.3.0",
    "tailwindcss": "4.1.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "stylelint": "16.19.1",
    "stylelint-config-standard": "38.0.0",
    "stylelint-config-tailwindcss": "1.0.0",
    "stylelint-prettier": "5.0.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5"
  }
}
